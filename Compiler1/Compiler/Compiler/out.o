global _main
 extern _printf 
segment .text
_main:push EBP 
mov EBP,Data
 add EBP,0xFF
 call KeyBoard_start
pop EBP
ret
KeyBoard_start:
sub ESP,0x2
mov byte ah,0x2
mov byte [EBP-0x1],ah
mov byte ah,0x3
mov byte [EBP-0x2],ah
mov byte al,[EBP-0x1]
movsx EAX,al
push EAX
push dword int_format
 call _printf
 pop EAX
pop EAX
mov byte ah,0x4
mov byte [EBP-0x2],ah
Epilogue_KeyBoard_start:
add ESP,0x2

ret 
segment .data
 Data resb 260 
int_format db 'Value=%d',0
 string_format  db 'String Value=%s',0 
 uint_format db 'Value=%d',0